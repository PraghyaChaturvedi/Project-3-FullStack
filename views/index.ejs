    <script>
        let text=document.querySelectorAll(".text");
        let inita=0;

        function slider(jumper){
            if (jumper >= text.length){
                jumper=0;
            }

                text[inita].classList.toggle("active");
                text[jumper].classList.toggle("active");  
                inita=jumper; 
        }


        let fouthPageGirl=document.querySelectorAll(".fouthPageGirl");
        let inita1=0
        function slider1(jumper){
            if (jumper >= fouthPageGirl.length){
                jumper=0;
            }

            fouthPageGirl[inita1].classList.toggle("activate");
            fouthPageGirl[jumper].classList.toggle("activate");
            inita1=jumper; 
        }



        window.onload=setInterval(function(){
            slider(inita+1);
            slider1(inita1+1);
        },2000)

        // hamburger

        let hamburger =document.querySelector(".hamburger");
        let hamburgerVisible=document.querySelector(".hamburgerVisible");
        let headLogo=document.querySelector(".headLogo");
        let ham=document.querySelector(".ham");

        hamburger.addEventListener("click", ()=>{
            hamburgerVisible.classList.remove("hider");
            headLogo.classList.add("hider");
            ham.classList.add("hider");

        });


        let closer =document.querySelector(".closer");

        closer.addEventListener("click", ()=>{
            hamburgerVisible.classList.add("hider");
            headLogo.classList.remove("hider");
            ham.classList.remove("hider");
        });

        function emailSent(){
            alert("Thanks for connecting with us!!");
        }

        function feedbackSent(){
            alert("Thanks for feedback!!");
        }
    </script>

